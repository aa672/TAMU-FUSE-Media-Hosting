<div>
<%= form_for @video_files, html: { multipart: true } do |f| %>   
    <%= f.label :title %>   
    <%= f.text_field :title %>   
    <br>
    <br>   
    <%= f.label :description %>   
    <%= f.text_field :description %>   
    <br><br>   
    <%= f.label :attachment %>   
    <%= f.file_field :attachment %>   
    <br>   
    <%= f.submit "Save" %>   
<% end %>
</div>