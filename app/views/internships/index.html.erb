<p id="notice"><%= notice %></p>

<h1>Internships</h1>

<table>
  <thead>
    <tr>
      <th>Internship</th>
      <th>Module sections</th>
      <th>Internship name</th>
      <th>Company name</th>
      <th>Contact name</th>
      <th>Contact email</th>
      <th>Company link</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @internships.each do |internship| %>
      <tr>
        <td><%= internship.internship_id %></td>
        <td><%= internship.module_sections_id %></td>
        <td><%= internship.internship_name %></td>
        <td><%= internship.company_name %></td>
        <td><%= internship.contact_name %></td>
        <td><%= internship.contact_email %></td>
        <td><%= internship.company_link %></td>
        <td><%= internship.description %></td>
        <td><%= link_to 'Show', internship %></td>
        <td><%= link_to 'Edit', edit_internship_path(internship) %></td>
        <td><%= link_to 'Destroy', internship, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Internship', new_internship_path %>
