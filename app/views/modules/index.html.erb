<p id="notice"><%= notice %></p>

<h1>Modules</h1>

<table>
  <thead>
    <tr>
      <th>Module</th>
      <th>Module name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @modules.each do |module| %>
      <tr>
        <td><%= module.module_id %></td>
        <td><%= module.module_name %></td>
        <td><%= link_to 'Show', module %></td>
        <td><%= link_to 'Edit', edit_module_path(module) %></td>
        <td><%= link_to 'Destroy', module, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Module', new_module_path %>
